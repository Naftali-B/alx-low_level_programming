#include <stdio.h>

int is_palindrome(int n)
{
    int rev = 0, num = n;

    while (num > 0) {
        rev = rev * 10 + num % 10;
        num /= 10;
    }

    return (n == rev);
}

int main()
{
    int i, j, largest_palindrome = 0;

    for (i = 999; i >= 100; i--) {
        for (j = 999; j >= 100; j--) {
            int product = i * j;
            if (is_palindrome(product) && product > largest_palindrome) {
                largest_palindrome = product;
            }
        }
    }

    printf("Largest palindrome made from the product of two 3-digit numbers is %d\n", largest_palindrome);

    return 0;
}

